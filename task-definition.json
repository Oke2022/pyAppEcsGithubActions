{
    "family": "python-app-task",
    "networkMode": "awsvpc",
    "executionRoleArn": "arn:aws:iam::241533137667:role/ecsTaskExecutionRole",
    "containerDefinitions": [
      {
        "name": "python-app",
        "image": "241533137667.dkr.ecr.us-east-1.amazonaws.com/python-app-demo/python-app-demo:latest",
        "essential": true,
        "portMappings": [
          {
            "containerPort": 5000,
            "hostPort": 5000,
            "protocol": "tcp"
          }
        ],
        "logConfiguration": {
          "logDriver": "awslogs",
          "options": {
            "awslogs-group": "/ecs/python-app",
            "awslogs-region": "us-east-1",
            "awslogs-stream-prefix": "ecs"
          }
        }
      }
    ],
    "requiresCompatibilities": ["FARGATE"],
    "cpu": "256",
    "memory": "512"
  }
